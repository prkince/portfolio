<div class="page-projects">
  <div class="projects-cards">
    <div class="container-projects-card" data-aos="fade-down" data-aos-delay="500">
      <%= link_to("https://shipantiques.herokuapp.com/", target: "_blank" , class: "project-link") do %>
        <div class="description">
          <div class="description-img"> <%= cl_image_tag("https://res.cloudinary.com/dovu27lye/image/upload/c_scale,w_250/v1580894285/portfolio/ship_antiques_accueil_e8mydf.png", :quality=>55) %></div>
          <div class="details">
            <h2><%= t '.sa_title' %></h2>
            <p>
              <%= t '.sa_text' %>
            </p>
          </div>
        </div>
      <% end %>
    </div>

